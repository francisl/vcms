// CONTENT
div#vcms_content:
    &>div:
        text-align: center;
    h1:
        font-weight: bolder;
        text-transform: uppercase;
    h3:
        font-weight: normal;
        padding: 0;
    span.black:
        color: #000;
    .form:
        width: 100%;
        float: left;
        margin-right: 2em;
        margin-bottom: 1em;
        text-align: center;
        table:
            border-spacing: 0;
    .submit:
        float: right;
    .notice:
        float: left;
